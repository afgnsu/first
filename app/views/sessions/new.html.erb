<h1>用戶登入</h1>
<%= form_for :session, url: sessions_path do |f| %>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
<table class="table table-hover">
  <tr><td><%= f.label :email, "電子信箱：" %><%= f.text_field :email %></td>
  <td><%= f.label :password, "密碼：" %><%= f.password_field :password %></td><td><%= f.submit "登入" %></td></tr>
</table>
</div>
</div>
<% end %>
